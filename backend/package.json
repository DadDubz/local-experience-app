{
  "name": "local-experience-app-backend",
  "version": "1.0.0",
  "description": "Backend for Local Experience App",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:integration": "jest --testMatch=\"**/__tests__/integration/**/*.test.js\"",
    "test:unit": "jest --testMatch=\"**/__tests__/unit/**/*.test.js\"",
    "lint": "eslint .",
    "build": "babel src -d dist && npm run sentry:sourcemaps",
    "sentry:sourcemaps": "sentry-cli sourcemaps inject --org local-experience-app --project node .github/workflows/quality.yml && sentry-cli sourcemaps upload --org local-experience-app --project node .github/workflows/quality.yml"
  },
  "dependencies": {
    "@sentry/cli": "^2.42.4",
    "axios": "^1.8.2",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^6.7.0",
    "helmet": "^7.0.0",
    "jsonwebtoken": "^9.0.0",
    "mongoose": "^7.3.1",
    "multer": "^1.4.5-lts.1",
    "socket.io": "^4.6.1",
    "validator": "^13.9.0",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.22.5",
    "@babel/core": "^7.22.5",
    "@babel/preset-env": "^7.22.5",
    "eslint": "^8.44.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.27.5",
    "jest": "^29.5.0",
    "mongodb-memory-server": "^8.12.2",
    "nodemon": "^2.0.22",
    "supertest": "^6.3.3"
  }
}